 confPass){\n            alert(\"passwords don't match\");\n            return;\n        }\n\n        try {\n            const { user } = await auth.createUserWithEmailAndPassword(email,password);\n            await createUserProfileDocument(user,{ displayName });\n\n            this.setState({\n                displayName: '',\n                email: '',\n                password: '',\n                confPass: ''\n            });\n        } catch (error) {\n            console.error(error);\n        }\n    }\n\n    handleChange = (evt) => {\n        const {name, value} = evt.target;\n        this.setState({[name]:value});\n    }\n\n\n    render(){\n        const {displayName, email, password, confPass} = this.state;\n        return(\n            <div className='sign-up'>\n                <h2 className='title'>I do not hava an account</h2>\n                <span>sign up with e-mail and password</span>\n                <form className='sign-up-form' onSubmit={this.handleSubmit}>\n                    <FormInput\n                    type='text'\n                    name='displayName'\n                    value={displayName}\n                    onChange={this.handleChange}\n                    label='Display Name'\n                    required\n                    />\n                    <FormInput\n                    type='email'\n                    name='email'\n                    value={email}\n                    onChange={this.handleChange}\n                    label='E-mail'\n                    required\n                    />\n                    <FormInput\n                    type='password'\n                    name='password'\n                    value={password}\n                    onChange={this.handleChange}\n                    label='Password'\n                    required\n                    />\n                    <FormInput\n                    type='password'\n                    name='confPass'\n                    value={confPass}\n                    onChange={this.handleChange}\n                    label='confirm password'\n                    required\n                    />\n                    <CustomButton type='submit'>Sign Up</CustomButton>\n                </form>\n            </div>\n        )\n    }\n}\n\n\nexport default SignUp;",["110","111"],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/redux/user/user.reducer.js",[],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/redux/root-reducer.js",[],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/redux/store.js",[],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/redux/user/user.actions.js",[],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/components/cart-icon/cart-icon.component.jsx",[],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/redux/cart/cart.types.js",[],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/redux/cart/cart.actions.js",[],"/home/yonibrese/Documents/wolf_pack/tutorials/react_udemy/big-project/src/redux/cart/cart.reducer.js",[],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"112","replacedBy":"116"},{"ruleId":"114","replacedBy":"117"},{"ruleId":"112","replacedBy":"118"},{"ruleId":"114","replacedBy":"119"},{"ruleId":"120","severity":1,"message":"121","line":5,"column":8,"nodeType":"122","messageId":"123","endLine":5,"endColumn":20},{"ruleId":"112","replacedBy":"124"},{"ruleId":"114","replacedBy":"125"},"no-native-reassign",["126"],"no-negated-in-lhs",["127"],["126"],["127"],["126"],["127"],"no-unused-vars","'CustomButtom' is defined but never used.","Identifier","unusedVar",["126"],["127"],"no-global-assign","no-unsafe-negation"]